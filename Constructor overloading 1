#include<iostream>
using namespace std;
class Student{
    string name;
    int age;
    public:
    Student(){
        name="Nia";
        age=18;
    }
    Student(string n ,int a){
        name=n;
        age=a;
        
    }
    void display(){
        cout<<"Name:"<<name<<endl<<"Age"<<age<<endl;
    }
};
int main(){
    Student s1;
    Student s2("Suyashi",19);
    s1.display();
    s2.display();
    return 0;
}
/*Output:
Name:Nia
Age18
Name:Suyashi
Age19


=== Code Execution Successful ===*/
